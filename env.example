# Docker compose file for local development
COMPOSE_FILE=docker-compose.dev.yml

# Bot
BOT_TOKEN=your_telegram_bot_token_here
BOT_INCIDENTS_USER_ID=your_telegram_user_id_here
BOT_ENVIRONMENT=local
BOT_DOMAIN=
BOT_WEBHOOK_URL=/telegram-webhook

# FlareSolverr
FLARESOLVERR_URL=http://listambot.flaresolverr:8191
FLARESOLVERR_PORT=8191
FLARESOLVERR_MAX_TIMEOUT=60000

# Environment
ENV=local
NODE_ENV=development

# Service
SERVICE_REST_PORT=3000

# Database => PostgreSQL
POSTGRES_HOST=listambot.postgres
POSTGRES_PORT=5032
POSTGRES_USERNAME=listambot
POSTGRES_PASSWORD=password
POSTGRES_NAME=listambot
POSTGRES_LOGGING=false
POSTGRES_BASE_URL=postgresql://${POSTGRES_USERNAME}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_NAME}
POSTGRES_TELEGRAF_SCHEMA=public

# Core database schema
CORE_POSTGRES_SCHEMA=core
CORE_POSTGRES_URL=${POSTGRES_BASE_URL}?schema=${CORE_POSTGRES_SCHEMA}
