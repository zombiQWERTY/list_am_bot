COMPOSE_FILE=docker-compose.dev.yml

BOT_TOKEN=your_telegram_bot_token_here

CRON_SCHEDULE=0 * * * *
FETCH_TIMEOUT_MS=15000
REQUEST_DELAY_MS=2500
MAX_RETRIES=3

LISTAM_BASE_URL=https://www.list.am

# 2CAPTCHA API KEY (for bypassing CAPTCHAs)
CAPTCHA_API_KEY=your_2captcha_api_key_here

# PROXY SETTINGS
# Residential proxies are loaded from servers.json in the project root
# Format in servers.json: "username__cr.country1,country2:password@host:port"

# Proxy rotation settings (optional, defaults are set in proxy.config.ts)
PROXY_ROTATION_BASE_MINUTES=15
PROXY_ROTATION_MAX_MINUTES=30
PROXY_ROTATION_JITTER_FRACTION=0.2

# Proxy retry settings (optional)
PROXY_RETRY_MAX_ATTEMPTS=3
PROXY_RETRY_BASE_DELAY_MS=500

# Proxy health check (optional)
PROXY_HEALTH_CHECK_TIMEOUT_MS=10000
PROXY_HEALTH_CHECK_MAX_FAILURES=3

LOG_LEVEL=info

ENV=local
NODE_ENV=development

SERVICE_REST_PORT=3000

# DATABASE => POSTGRES
POSTGRES_HOST=list_am_bot.postgres
POSTGRES_PORT=5032
POSTGRES_USERNAME=list_am_bot
POSTGRES_PASSWORD=password
POSTGRES_NAME=list_am_bot
POSTGRES_LOGGING=false
POSTGRES_BASE_URL=postgresql://${POSTGRES_USERNAME}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_NAME}

CORE_POSTGRES_SCHEMA=core
CORE_POSTGRES_URL=${POSTGRES_BASE_URL}?schema=${CORE_POSTGRES_SCHEMA}
